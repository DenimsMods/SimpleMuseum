{
  "schemaVersion": 1,
  "id": "simplemuseum",
  "version": "${version}",
  "name": "Simple Museum",
  "description": "Adds configurable puppet entities to display custom models and act as custom NPCs, among other purposes.",
  "authors": [
    {
      "name": "DenimRed",
      "contact": {
        "homepage": "https://github.com/DenimRed"
      }
    }
  ],
  "license": "MIT",
  "contact": {
    "homepage": "https://github.com/DenimsMods/SimpleMuseum",
    "sources": "https://github.com/DenimsMods/SimpleMuseum/tree/1.20.1",
    "issues": "https://github.com/DenimsMods/SimpleMuseum/issues"
  },
  "icon": "assets/simplemuseum/icon.png",
  "entrypoints": {
    "main": [
      "dev.denimred.simplemuseum.SimpleMuseum"
    ],
    "client": [
      "dev.denimred.simplemuseum.SimpleMuseumClient"
    ]
  },
  "mixins": [
    {
      "config": "simplemuseum.mixins.json",
      "environment": "*"
    },
    {
      "config": "simplemuseum.client.mixins.json",
      "environment": "client"
    }
  ],
  "accessWidener": "simplemuseum.accesswidener",
  "depends": {
    "fabricloader": ">=0.15.11",
    "minecraft": "~1.20.1",
    "java": ">=17",
    "fabric-api": "*",
    "geckolib": ">=4.4.7"
  }
}